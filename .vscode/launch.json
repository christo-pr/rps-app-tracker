    "version": "0.2.0",
    "configurations": [
      {
        "name": "Launch on iOS",
        "type": "nativescript",
        "request": "launch",
        "platform": "ios",
        "appRoot": "${workspaceRoot}",
        "sourceMaps": true,
        "watch": true
      },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}/references.d.ts",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        }
    ]
}
